{
  "dashboard": {
    "title": "Iris Model Monitoring",
    "panels": [
      {
        "type": "graph",
        "title": "Request Rate",
        "targets": [{"expr": "rate(iris_requests_total[1m])"}]
      },
      {
        "type": "graph",
        "title": "P95 Latency",
        "targets": [{"expr": "histogram_quantile(0.95, sum(rate(iris_inference_seconds_bucket[5m])) by (le))"}]
      },
      {
        "type": "piechart",
        "title": "Class Distribution",
        "targets": [{"expr": "sum(rate(iris_predictions_total[5m])) by (class)"}]
      },
      {
        "type": "heatmap",
        "title": "Sepal Length Distribution",
        "targets": [{"expr": "histogram_quantile(0.5, sum(rate(iris_sepal_length_bucket[5m])) by (le))"}]
      }
    ],
    "schemaVersion": 16,
    "version": 0,
    "refresh": "10s"
  },
  "overwrite": true
}